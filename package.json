{
  "name": "novate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:prod": "npm run remove-console && next build",
    "remove-console": "find . -name '*.ts' -o -name '*.tsx' -o -name '*.js' -o -name '*.jsx' | grep -v node_modules | grep -v .next | xargs sed -i.bak 's/console\\.log(/\\/\\/ console.log(/g' && find . -name '*.ts' -o -name '*.tsx' -o -name '*.js' -o -name '*.jsx' | grep -v node_modules | grep -v .next | xargs sed -i.bak 's/console\\.debug(/\\/\\/ console.debug(/g' && find . -name '*.ts' -o -name '*.tsx' -o -name '*.js' -o -name '*.jsx' | grep -v node_modules | grep -v .next | xargs sed -i.bak 's/console\\.info(/\\/\\/ console.info(/g'",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "rm -rf .next out dist",
    "clean:all": "rm -rf .next out dist node_modules package-lock.json pnpm-lock.yaml",
    "install:clean": "npm run clean:all && npm install",
    "analyze": "ANALYZE=true npm run build",
    "export": "next export",
    "production-check": "npm run type-check && npm run lint && npm run test",
    "prebuild": "npm run type-check",
    "postbuild": "echo 'Build completed successfully!'"
  },
  "dependencies": {
